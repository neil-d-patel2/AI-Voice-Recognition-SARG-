__pycache__/
*.py[cod]
*$py.class
*.so
.Python

# Virtual environments
venv/
env/
ENV/

# IDE settings
.vscode/
.idea/
.DS_Store
*.swp

# Audio files (large binaries)
*.mp3
*.wav
*.m4a

# Object/compiled files - HIDE THESE SPECIFIC FILES
play
parse_play

# Jupyter notebooks - HIDE THIS SPECIFIC FILE
test.ipynb

# Jupyter checkpoints
.ipynb_checkpoints/
*.ipynb_checkpoints

#recorder.py (currently unused)
recorder.py

# Logs and databases
*.log
*.db
*.sqlite

# Distribution / packaging
dist/
build/
*.egg-info/
EOF